{
  "name": "serverless-cron",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "serverless",
    "cron",
    "scheduler",
    "schedule"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup-node src/index.ts --dts --format esm,cjs --legacy-output --target es2017,node14 --minify --treeshake --clean --out-dir dist",
    "prepublish": "pnpm run build",
    "postversion": "git push --follow-tags && pnpm publish"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cron-parser": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}